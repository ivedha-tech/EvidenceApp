{
  "manifest_version": 3,
  "name": "Screenshot Extension",
  "version": "1.0",
  "description": "A simple extension to take screenshots",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://mail.google.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
} 